{
  "name": "@lbgm/ncom",
  "version": "9.0.2-g",
  "author": "@lbgm",
  "description": "JS dialog box",
  "main": "build/src/index.js",
  "exports": {
    ".": {
      "import": "./build/src/index.js",
      "require": "./build/src/index.js"
    },
    "./style": {
      "import": "./build/src/assets/ncom.css",
      "require": "./build/src/assets/ncom.css"
    },
    "./cdn": {
      "import": "./build/src/c.js",
      "require": "./build/src/index.js"
    }
  },
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "vuejs",
    "angular",
    "dialog box",
    "typescript",
    "javascript",
    "typescript dialog box"
  ],
  "engines": {
    "node": ">=14.18.3",
    "npm": ">=6.14.15"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc && cp -R ./src/assets build/src/.",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@types/node": "^14.18.34",
    "eslint": "^7.32.0",
    "gts": "^3.1.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbgm/ncom.git"
  },
  "bugs": {
    "url": "https://github.com/lbgm/ncom/issues"
  },
  "homepage": "https://github.com/lbgm/ncom#readme"
